/**app.wxss**/
@import "common/template/wetoast/wetoast.wxss";

page{
  width:100%;
  height:100%;
}
view{
  font-family: PingFangSC;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.head-line-view{
  position: fixed;
  margin-left: 0rpx;
  margin-right: 0rpx;
  float: right;
  top: 0rpx;
  z-index: 999;
  height: 2rpx;
  width: 100%;
  box-shadow: 0 3px 8px 0 rgba(235, 235, 235, 0.3), inset 0 -1px 0 0 rgba(218, 218, 218, 0.5);
}
