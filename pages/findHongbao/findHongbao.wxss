page {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    background-color: #f7f7f7;
}

.btn-around {
    position: fixed;
    bottom: 100rpx;
    left: 0;
    width: 111rpx;
    height: 96rpx;
}

.btn-list {
    position: fixed;
    bottom: 100rpx;
    right: 0;
    width: 111rpx;
    height: 96rpx;
}

.hide-scroll {
    overflow: hidden;
}

.list-mode {
    box-sizing: border-box;
    padding: 0 0 100rpx;
    position: relative;
    height: 100%;
}

.news {
    position: absolute;
    top: 24rpx;
    left: 38rpx;
    max-width: 85%;
    height: 40rpx;
    padding-right: 10rpx;
    border-radius: 20rpx;
    background-color: #000000;
    z-index: 10;
}

.news .avatar {
    position: absolute;
    top: 0;
    left: 0;
    width: 40rpx;
    height: 40rpx;
    background-color: #ddd;
    border-radius: 50%;
}

.news .message {
    padding-right: 10rpx;
    margin-left: 50rpx;
    line-height: 40rpx;
    font-size: 20rpx;
    color: #ffffff;
    text-overflow: ellipsis;
    /* NOTE：BOTH of the following are required for text-overflow */
    white-space: nowrap;
    overflow: hidden;
}

.banner {
    position: relative;
    width: 100%;
    height: 450rpx;
}

.banner .bg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
}

.banner .action {
    display: flex;
    justify-content: center;
    width: 100%;
