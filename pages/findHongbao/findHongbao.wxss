}

.banner .btn-new-money {
    width: 330rpx;
    height: 289rpx;
    padding-right: 7rpx;
}

.banner .btn-luck {
    width: 330rpx;
    height: 289rpx;
    padding-left: 7rpx;
}

.data {
    display: flex;
    justify-content: space-around;
    align-items: center;
    position: absolute;
    top: 375rpx;
    left: 50%;
    transform: translate(-50%, 0);
    width: 690rpx;
    height: 150rpx;
    background: #fff;
    border-radius: 2rpx;
    box-shadow: 0 2rpx 16rpx 0 rgba(0, 0, 0, 0.08);
}

.data .value {
    font-size: 40rpx;
