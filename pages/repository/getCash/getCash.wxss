
page{
  background-color: rgb(247, 247, 247);
}

.get-cash{
  width:100%;
  height:100%;
}
.input-money{
  width:100%;
  height:300rpx;
  box-shadow: inset 0 -1rpx 0 0 #d9d9d9;
  background:#fff;
}
.input-money .input-money-tip{
  display: flex;
  width:100%;
  padding-top: 30rpx;
  flex-direction: column;
}
.input-money .input-money-tip .title{
  margin-left:30rpx;
  font-size:26rpx;
  color: #2b2b2b;
}
.input-money .input-money-tip .tip{
  margin-top: 5rpx;
  margin-left:30rpx;
  font-size:25rpx;
  color: rgb(160, 160, 160);
}

.input-money .input-money-content{
  display: flex;
  align-items: center;
  width:100%;
  height:120rpx;
  margin-top:20rpx;
}
.input-money .input-money-content text{
  margin-left:25rpx;
  font-size:50rpx;
  color: #2b2b2b;
}
.input-money .input-money-content input{
  flex: 1;
  height:100rpx;
  padding-left:20rpx;
  padding-right: 10rpx;
  font-size: 50rpx;
}


/* 手续费 */
.handicap-cost-text{
  display: flex;
  margin-top: 20rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  font-size:26rpx;
  color: #2b2b2b;
}


/*  提现按钮  */
.submit-default{
  margin:0 auto;
  width:650rpx;
  height:88rpx;
  margin-top:170rpx;
  text-align: center;
  line-height:88rpx;
  font-size:36rpx;
  color:#fff;
  background:#dcdcdc;
  border-radius:6rpx;
}
.submit-active{
  margin:0 auto;  
  width:650rpx;
  height:88rpx;
  margin-top:170rpx;
  text-align: center;
  line-height:88rpx;
  font-size:36rpx;
  color:#fff;
  background:#ee4126;
  border-radius:6rpx;
}